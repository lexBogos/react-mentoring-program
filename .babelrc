{
    "presets": [
      [
        "@babel/preset-env",
        {
            "targets": {
              "chrome": "70",
              "ie": "11",
              "safari": "10",
              "firefox": "60"
            }
          }
      ],
      [
        "@babel/preset-react"
      ]
    ],
    "env": {
      "test": {
        "plugins": ["@babel/plugin-transform-modules-commonjs", ["@babel/plugin-transform-runtime",
          {
            "regenerator": true
          }
        ]]
      }
    }
  }
  